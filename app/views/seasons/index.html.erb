<h1>Listing seasons</h1>

<table>
  <tr>
    <th>From</th>
    <th>To</th>
    <th>Winners</th>
    <th>Mos</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seasons.each do |season| %>
  <tr>
    <td><%= season.from %></td>
    <td><%= season.to %></td>
    <td><%= season.winner.name if season.winner %></td>
    <td><%= season.mos.name if season.mos %></td>
    <td><%= link_to 'Fixture', season_matches_path(season) %></td>
    <td><%= link_to 'Player Bonds', season_playerbonds_path(season) %></td>
    <!-- <td><%= link_to 'Show', season %></td> -->
    <td><%= link_to 'Edit', edit_season_path(season) %></td>
    <td><%= link_to 'Destroy', season, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Season', new_season_path %>
